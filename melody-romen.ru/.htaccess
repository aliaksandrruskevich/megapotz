AddDefaultCharset windows-1251
DefaultLanguage ru
AddType video/mp4 .mp4
AddType video/ogg .ogv
FileETag None

RewriteEngine on
#RewriteCond %{HTTP_HOST}   !^melody-romen\.ru [NC]
#RewriteRule ^/         http://www.melody-romen.com/ [L,R=301]

RewriteCond %{HTTP_HOST} ^melody-romen\.ru
RewriteRule ^(.*)$ http://www.melody-romen.ru/$1 [R=permanent,L]

<ifModule mod_headers.c>
	Header unset ETag
	Header unset Last-Modified
	Header set Cache-Control "public, max-age=592200"
</ifModule>

<IfModule mod_expires.c>
	ExpiresActive On
	ExpiresDefault "access plus 4 hours"
	ExpiresByType application/javascript A592200
	ExpiresByType application/x-javascript A592200
	ExpiresByType text/html A592200
</IfModule>