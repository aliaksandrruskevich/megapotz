AddDefaultCharset utf-8
DefaultLanguage ru
FileETag None

RewriteEngine on

#RewriteCond %{HTTP_USER_AGENT} Googlebot
#RewriteCond %{HTTP_HOST} otdelkalux\.ru
#RewriteRule ^(.*)$ http://xn----htbbbbfwdqkkdt6ada.xn--p1ai/$1 [R=301,NC,L]

RewriteCond %{HTTP_HOST} ^otdelkalux\.ru
RewriteRule ^(.*)$ http://www.otdelkalux.ru/$1 [R=301,L]

<ifModule mod_headers.c>
	Header unset ETag
</ifModule>

<IfModule mod_expires.c>
  ExpiresActive on
  ExpiresDefault                          "access plus 1 month"
  ExpiresByType text/html                 "access plus 1 day"
</IfModule>