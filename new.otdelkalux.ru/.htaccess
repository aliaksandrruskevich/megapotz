AddDefaultCharset utf-8
DefaultLanguage ru
FileETag None

RewriteEngine on
RewriteCond %{HTTP_HOST} ^otdelkalux\.ru [OR]
RewriteCond %{HTTP_HOST} \.xn--p1ai$
RewriteRule ^(.*)$ http://www.otdelkalux.ru/$1 [R=301,L]

Redirect 301 /portfolio/process/ http://www.otdelkalux.ru/osmotr/

<ifModule mod_headers.c>
	Header unset ETag
	BrowserMatch MSIE ie
	Header set X-UA-Compatible "IE=Edge,chrome=1" env=ie
</ifModule>

<IfModule mod_expires.c>
  ExpiresActive on
  ExpiresDefault                          "access plus 1 month"
  ExpiresByType text/html                 "access plus 1 day"
</IfModule>